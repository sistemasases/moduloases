{{#data}} {{> block_ases/side_menu}} {{{message}}}
<div class="container">
  <div id="custom_metadata" data-view-name="student_profile"></div>
  {{#sistemas}}<div id="dphpforms_role_support" data-info="sistemas" style="display:none;"></div>{{/sistemas}} 
  {{#dir_socioeducativo}}<div id="dphpforms_role_support" data-info="dir_socioeducativo" style="display:none;"></div>{{/dir_socioeducativo}} 
  {{#monitor_ps}}<div id="dphpforms_role_support" data-info="monitor_ps" style="display:none;"></div>{{/monitor_ps}}
  <div id="dphpforms_ases_student_code" data-info="{{ases_student_code}}" style="display:none;"></div>
  <div id="dphpforms_block_instance" data-info="{{instance}}" style="display:none;"></div>
  <div id="dphpforms_block_courseid" data-info="{{courseid}}" style="display:none;"></div>
  <div id="dphpforms_monitor_id" data-info="{{monitor_id}}" style="display:none;"></div>
  <div id="dphpforms_practicing_id" data-info="{{practicing_id}}" style="display:none;"></div>
  <div id="dphpforms_professional_id" data-info="{{professional_id}}" style="display:none;"></div>
  <div id="dphpforms_username" data-info="{{student_username}}" style="display:none;"></div>
  <div id="dphpforms_fullname" data-info="{{student_fullname}}" style="display:none;"></div>
  <div id="cod_programa_activo" data-info="{{cod_programa_activo}}" style="display:none;"></div>
  <div id="risk_graphic_info" data-info="" style="display:none;"></div>
  <input type="text" class="hide" id="current_user_id" value="{{id_dphpforms_creado_por}}"/>
  
  <form id="ficha_estudiante">

    <input type="text" class="hide" id="id_ases" name="id_ases" value="{{id_ases}}" />
    <input type="text" class="hide" id="id_moodle" name="id_moodle" value="{{id_moodle}}" />

    <div class="row" id="div-header-profile">
      <div class="col-xs-10 col-sm-10 col-md-10 col-lg-10 " id="div-header-info">

        <!-- Select student -->   
        <div class="row" id="div-code-student">

          <div class="col-xs-11 col-sm-11 col-md-11 col-lg-11" id="div_select_student">
            {{{code}}}
          </div>

          <div class="col-sm-1" id="div-instance-icon">
            <span class="glyphicon glyphicon-ok i-tracking{{#ases_status_on_tracking}}-t{{/ases_status_on_tracking}} tooltip-v1 col-sm-6"
              {{#update_ases_status_sp}} id="icon-tracking" {{/update_ases_status_sp}} data-toggle="tooltip" data-placement="top"
              title="{{ases_status_description}}"></span>
            <input type="hidden" id="input_status_ases" value="{{ases_status}}">
            <span>ASES</span>
          </div>
        </div>
        <!-- End select student -->

        <div class="row" id="row-general-fields">

          <div class="col-xs-1 col-sm-1 col-md-1 col-lg-1 div_general_fields" id="div_doc_type">
            <select name="tipo_doc" class="form-control" id="i_tipo_doc" name="tipo_doc" disabled>
              {{#document_type}}
              <option value="{{nombre}}" {{selected}}>{{nombre}}</option>
              {{/document_type}}
            </select>
          </div>

          <div class="col-xs-1 col-sm-1 col-md-1 col-lg-1 div_general_fields" id="div_input_cc">
              <input type="text" class="form-control" name="num_doc" id="num_doc" value="{{num_doc}}" maxlength="12" readonly>
          </div>

          <div class="col-xs-1 col-sm-1 col-md-1 col-lg-1 div-general-fields" id="div_age">
              <span id="age">{{age}} años</span>
              <input type="text" id="birthdate" name="fecha_nac" class="brithdate_picker" maxlength="10" value="{{birthdate}}"/>
          </div>

          <div class="col-xs-5 col-sm-5 col-md-5 col-lg-5 div-general-fields" id="div_email_moodle">
              <input type="email" class="form-control" name="email" id="email" value="{{email_moodle}}" readonly>
          </div>

          <div class="col-xs-1 col-sm-1 col-md-1 col-lg-1 div-general-fields">
            <span id="span-icetex-status">ICETEX</span>
          </div>

          <div class="col-xs-2 col-sm-2 col-md-2 col-lg-2 div-general-fields" id="div_icetex_status">
            {{#update_icetex_status_sp}}
            <select id="icetex_status" class="form-control" disabled>
              {{{options_status_icetex}}}
            </select>
            {{/update_icetex_status_sp}} {{^update_icetex_status_sp}}
            <span class="col-sm-2 tooltip-v1" id="span-icetex-name" ; data-toggle="tooltip" data-placement="top" title="{{icetex_status_name}}">
              {{icetex_status_description}}
            </span>
            {{/update_icetex_status_sp}}
          </div>
        </div>

        <div class="row" id="div-hr">
          <hr class="hr-general-fields">
        </div>

        <div class="col-xs-12 col-sm-8 col-md-8 col-lg-8" id="div_academic_programs">
          <h4>Programas académicos</h4>
          <table class="table table-hover borderless">
            <tbody>
              {{#academic_programs}}
              <tr id="tr-{{id_moodle_user}}">
                <td class="col-sm-8 td-middle" id="{{academic_program_id}}">
                  {{{cod_univalle}}} - {{{nombre_programa}}} - {{{nombre_sede}}}
                </td>
                <td class="col-sm-3">
                  {{#update_program_status_sp}}
                  <select class="select_statuses_program" id="select-{{academic_program_id}}">
                    {{/update_program_status_sp}} {{^update_program_status_sp}}
                    <select class="select_statuses_program" id="select-{{academic_program_id}}" disabled>
                      {{#statuses}}
                      <option value={{id}} {{selected}}>{{nombre}}</option>
                      {{/statuses}}
                    </select>
                    {{/update_program_status_sp}}
                  </select>
                </td>
                <td class="col-sm-1 td-middle">
                  {{#update_tracking_status_sp}}
                  <label class="switch">
                    <input class="input-tracking" type="checkbox" id="div_flags_{{academic_program_id}}" disabled>
                    <div class="slider round"></div>
                  </label>
                  {{/update_tracking_status_sp}}
                </td>
              </tr>
              {{/academic_programs}}
            </tbody>
          </table>
          {{^academic_programs}} No registra programas académicos. {{/academic_programs}}
        </div>

        <div class="col-xs-12 col-sm-3 col-md-3 col-lg-3" id="div_cohorts">
          <h4>Cohortes</h4>
          <table class="table table-sm table-hover borderless" id="cohorts_table">
            <tbody>
              {{#student_cohorts}}
              <tr>
                <td class="col-xs-12 col-sm-12 td-middle">
                  {{name}}
                </td>
              </tr>
              
              {{/student_cohorts}}
            </tbody>
          </table>
          <div id="cond_excepcion_name" hidden>
            {{condition_excepcion}}
          </div>
          <div id="cond_excepcion_alias" hidden>
            {{condition_excepcion_alias}}
          </div>
          
          {{^student_cohorts}} No registra cohortes asociadas. {{/student_cohorts}}
        </div>
        {{#update_general_tab_sp}}

        <div class="col-xs-1 col-sm-1 col-md-1 col-lg-1" id="div-icon-edit">
          <span class="col-sm-12 tooltip-v1" id="span-icon-edit" data-toggle="tooltip" data-placement="top" title="Editar ficha">
          </span>

          <span class="col-sm-6 tooltip-v1" id="span-icon-save-profile" data-toggle="tooltip" data-placement="top" title="Guardar cambios">
          </span>

          <span class="col-sm-6 tooltip-v1" id="span-icon-cancel-edit" data-toggle="tooltip" data-placement="top" title="Cancelar edición">
          </span>
        </div>
        {{/update_general_tab_sp}}

      </div>
      <!-- Profile image html, see $OUTPUT->user_picture -->
      <div class="col-xs-2 col-sm-2 col-md-2 col-lg-2  {{#show_html_elements_update_user_profile_image}}profile-image-can-update{{/show_html_elements_update_user_profile_image}} profile-image">
        {{{html_profile_image}}}
        {{#show_html_elements_update_user_profile_image}}
        <div class="open-profile-image">
         <button type=button id="view_form_update_profile_image" class="open-button"> Cambiar imágen de perfil </button>
        </div>
        {{/show_html_elements_update_user_profile_image}}
      </div>
    </div>

    <!-- Semaforo de riesgos -->
    <div class="row div_risk">
      <div class="col-sm-1 no-padding-left">
        <div id="div_title_risk">
          <b>RIESGOS:</b>
        </div>
      </div>
      <div class="col-sm-9 no-padding-left" id="div_bar_risks">
        <div class="{{individual_class}} col-sm-2" data-risk-level="{{level_individual}}" id="individual_risk">
          <span><b>INDIVIDUAL</b></span>
        </div>
        <div hidden>{{individual_risk}}</div>

        <div class="{{familiar_class}} col-sm-2" data-risk-level="{{level_familiar}}" id="familiar_risk">
          <b> <span>FAMILIAR</span></b>
        </div>
        <div hidden>{{familiar_risk}}</div>

        <div class="{{academic_class}} col-sm-2" data-risk-level="{{level_academic}}" id="academic_risk">
          <b><span>ACADÉMICO</span></b>
        </div>
        <div hidden>{{academic_risk}}</div>

        <div class="{{economic_class}} col-sm-2" data-risk-level="{{level_economic}}" id="economic_risk">
          <b><span>ECONÓMICO</span></b>
        </div>
        <div hidden>{{economic_risk}}</div>

        <div class="{{life_class}} col-sm-2" data-risk-level="{{level_life}}" id="life_risk">
          <b>VIDA UNIV.</b>
        </div>
        <div hidden>{{life_risk}}</div>

        <div class="{{geographic_class}} col-sm-2" data-risk-level="{{geographic_risk_level}}" id="geo_risk">
          <b><span>GEOGRÁFICO</span></b>
        </div>
        <div hidden>{{geographic_risk_level}}</div>

      </div>
      <div class="col-sm-2" id="div_buttons_risk">
        <button class="risk_graph_button col-sm-6" type="button" id="view_graph_radial_button">Radial</button>
        <button class="risk_graph_button col-sm-6" type="button" id="view_graphic_risk_button">Trayectoria</button>
      </div>
    </div>

    <div class="row cabecera" id="container_profile">
      <!-- Pestañas ficha -->
      <div id="exTab1" class="col-sm-12">
        <ul class="nav nav-tabs" id="nav-linea">
          {{#view_general_tab_sp}}
          <li id="general_li" class="active">
            <a href="#general_tab" data-toggle="tab">GENERAL</a>
          </li>
          {{/view_general_tab_sp}} {{#view_socioed_tab_sp}}
          <li id="socioed_li">
            <a href="#socioed_tab" data-toggle="tab">SOCIOEDUCATIVO</a>
          </li>
        
  
          {{/view_socioed_tab_sp}} {{#view_academic_tab_sp}}
          <li id="academic_li" style="background: #cd1f32;">
            <a href="#academic_tab" data-toggle="tab" style="color: white;">ACADÉMICO</a>
          </li>
          {{/view_academic_tab_sp}} {{#view_geographic_tab_sp}}
          <li id="geographic_li">
            <a href="#geographic_tab" data-toggle="tab">GEOGRÁFICO</a>
          </li>{{/view_geographic_tab_sp}}
        
          {{#show_discapacity}}
            {{#view_others_tab_sp}}
            <li id="tracing_others_li">
              <a href="#tracing_others_tab" data-toggle="tab">CARACTERIZACIÓN</a>
            </li>
            {{/view_others_tab_sp}}
          {{/show_discapacity}} 

          {{#show_discapacity}}
            <li id="discapacity_tracking_li">
              <a href="#view_discapacity_tracking_tab" data-toggle="tab">AJUSTES RAZONABLES</a>
            </li>
          {{/show_discapacity}} 
       
        </ul>
        <div class="tab-content clearfix">
          {{#view_general_tab_sp}} {{> block_ases/view_general_tab_sp}} {{/view_general_tab_sp}}
          {{#view_others_tab_sp}} {{> block_ases/view_others_tab_sp}} {{/view_others_tab_sp}}
        </div>
      </div>
    </div>
  </form>

  <!-- MODALES DE SEGUIMIENTO -->
  <div class="mymodal" id="modal_v2_peer_tracking" name="modal_v2_pares">
    <div class="outside"></div>
    <div class="mymodal-content">

      <div class="mymodal-header col-sm-12">
        <div class="col-xs-9 col-sm-9 col-md-9 col-lg-9">
          <h4 class="" id="myModalLabel">Nuevo registro</h4>
        </div>
        <div class="col-xs-2 col-sm-2 col-md-2 col-lg-2">
          <div data-info="seguimiento" class="btn-inasistencia open-inasistencia btn btn-md btn-default">
            Registrar inasistencia
          </div>
        </div>
        <div class="col-xs-1 col-sm-1 col-md-1 col-lg-1">
          <span id="mymodal-v2-close" class="mymodal-close" aria-hidden="true">&times;</span>
        </div>
      </div>
      <div class="mymodal-body mymodal-body_v2 col-sm-12">
        <div class="row">
          <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
            {{{form_seguimientos}}}
          </div>
        </div>
      </div>
      <div class="mymodal-footer">
        .
      </div>
    </div>

  </div>
</div>

<div class="mymodal" id="modal_v2_edit_peer_tracking" name="modal_edit_v2_pares">
  <div class="outside"></div>
  <div class="mymodal-content">

    <div class="mymodal-header col-sm-12">
