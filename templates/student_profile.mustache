{{#data}} 
{{> block_ases/side_menu}} 
{{{message}}}

<div class="container">
  {{#sistemas}}<div id="dphpforms_role_support" data-info="sistemas" style="display:none;"></div>{{/sistemas}}
  {{#dir_socioeducativo}}<div id="dphpforms_role_support" data-info="dir_socioeducativo" style="display:none;"></div>{{/dir_socioeducativo}}
  <input type="text" class="hide" id="current_user_id" value="{{id_dphpforms_creado_por}}" />

  <form id="ficha_estudiante">
  
    <input type="text" class="hide" id="id_ases" name="id_ases" value="{{id_ases}}" />
    <input type="text" class="hide" id="id_moodle" name="id_moodle" value="{{id_moodle}}" />

    <div class="row" id="div-header-profile">
      <div class="col-xs-12 col-sm-10 col-md-10 col-lg-10 equalize" id="div-header-info">
        <div class="row" id="div-select-student">
          <div class="col-xs-11 col-sm-11">
            {{{code}}}
          </div>
          <div class="col-xs-1 col-sm-1">
            <div class="icontip">
              <span class="glyphicon glyphicon-ok {{#ases_status_t}}i-tracking-t{{/ases_status_t}}{{#ases_status_f}}i-tracking-f{{/ases_status_f}}{{#ases_status_n}}i-tracking-n{{/ases_status_n}}" {{#update_ases_status_sp}} id="icon-tracking" {{/update_ases_status_sp}}></span>
              <span class="icontiptext" id="tip_ases_status">{{ases_status_description}}</span>
              <input type="hidden" id="input_status_ases" value="{{ases_status_t}}{{ases_status_f}}{{ases_status_n}}">
            </div>
          </div>
        </div>
        <div class="row" id="row-general-fields">
          <div class="col-xs-2 col-sm-1 div-general-fields" id="">
            <select name="tipo_doc" class="" id="tipo_doc" name="tipo_doc" disabled>
              <option value="T.I" {{doc_type_ti}}>T.I.</option>
              <option value="C.C" {{doc_type_cc}}>C.C.</option>
              <option value="C.R" {{doc_type_cr}}>C.R.</option>
              <option id="EMPTY" {{doc_type_empty}}></option>
            </select>
          </div>
          <div class="col-xs-3 col-sm-2 div-general-fields">
            <input type="text" class="" name="num_doc" id="cedula" value="{{num_doc}}" size="12" maxlength="12" readonly>
          </div>
          <div class="col-xs-1 col-sm-1 div-general-fields">
            <span id="age">{{age}} años</span>
          </div>
          <div class="col-xs-3 col-sm-3 div-general-fields">
            <span id="email">{{email_moodle}}</span>
          </div>
          <div class="col-xs-2 col-sm-2 div-general-fields">
            <span id="span-icetex-status">Estado ICETEX</span>
          </div>
          <div class="col-xs-2 col-sm-2 div-general-fields">
            <select id="icetex_status" class="col-sm-10">
              {{{options_status_icetex}}}
            </select>
          </div>
        </div>
        <hr id="hr_header">
        <div class="col-xs-12 col-sm-8" id="div_academic_programs">
          <h4>Programas académicos</h4>
          <table class="table table-hover borderless">
            <tbody>
            {{#academic_programs}}
              <tr id="tr-{{id_moodle_user}}">
                <td class="col-sm-8 td-middle" id="{{academic_program_id}}">
                  {{cod_univalle}} - {{nombre_programa}}    
                </td>
                <td class="col-sm-3">
                  {{#update_program_status_sp}}
                  <select class="select_statuses_program" id="select-{{academic_program_id}}">
                  {{/update_program_status_sp}}
                  {{^update_program_status_sp}}
                  <select class="select_statuses_program" id="select-{{academic_program_id}}" disabled>
                  {{/update_program_status_sp}}
                      <option value="ACTIVO">ACTIVO</option>
                      <option value="APLAZADO">APLAZADO</option>
                      <option value="EGRESADO">EGRESADO</option>
                      <option value="INACTIVO">INACTIVO</option>
                      <option value="NOREGISTRA">NO REGISTRA</option>
                      <option value="RETIRADO">RETIRADO</option>
                  </select>
                </td>
                <td class="col-sm-1 td-middle">
                {{#update_tracking_status_sp}}
                  <label class="switch">
                    <input class="input-tracking" type="checkbox" id="div_flags_{{academic_program_id}}">
                    <div class="slider round"></div>
                  </label>
                {{/update_tracking_status_sp}}
                </td>
              </tr>
            {{/academic_programs}}
            </tbody>
          </table>
          {{^academic_programs}}
            No registra programas académicos.
          {{/academic_programs}}
        </div>
        <div class="col-xs-12 col-sm-4" id="div_cohorts">
          <h4>Cohortes</h4>
          <table class="table table-sm table-hover borderless">
            <tbody>
              {{#student_cohorts}}
                <tr>
                  <td class="col-xs-12 col-sm-12 td-middle">
                    {{name}}
                  </td>
                </tr>
              {{/student_cohorts}}
            </tbody>
          </table>
          {{^student_cohorts}}
            No registra cohortes asociadas.
          {{/student_cohorts}}
        </div>
      </div>
      <div class="col-xs-4 col-sm-2 col-md-2 col-lg-2 equalize" id="photo_div" style="background-image: url('../../../user/pix.php/{{id_user}}/f1.jpg'); background-repeat: no-repeat; background-size: 100% 100%;">
      </div>
    </div>

    <div class="row div_risk">
      <div class="col-sm-1 no-padding-left">
        <div id="div_title_risk">
          <b>RIESGOS:</b>
        </div>
      </div>
      <div class="col-sm-9 no-padding-left" id="div_bar_risks">
         <div class="{{individual_class}} col-sm-2" id="individual_risk">
            <span><b>INDIVIDUAL</b></span>
         </div>
         <div hidden>{{individual_risk}}</div>
         <div class="{{familiar_class}} col-sm-2" id="familiar_risk">
            <b> <span>FAMILIAR</span></b>
         </div>
         <div hidden>{{familiar_risk}}</div>
         <div class="{{academic_class}} col-sm-2" id="academic_risk">
            <b><span>ACADÉMICO</span></b>
         </div>
         <div hidden>{{academic_risk}}</div>
         <div class="{{economic_class}} col-sm-2" id="economic_risk">
            <b><span>ECONÓMICO</span></b>
         </div>
         <div hidden>{{economic_risk}}</div>
         <div class="{{life_class}} col-sm-2" id="life_risk">
            <b>VIDA UNIV.</b>
         </div>
         <div hidden>{{life_risk}}</div>
         <div class="{{geographic_class}} col-sm-2" id="geo_risk">
            <b><span>GEOGRÁFICO</span></b>
         </div>
         <div hidden>{{geographic_risk}}</div>
      </div>
      <div class="col-sm-2" id="div_buttons_risk">
        <button class="risk_graph_button col-sm-6" type="button" id="view_graphic_button">Radial</button>
        <button class="risk_graph_button col-sm-6" type="button" id="view_graphic_risk_button">Trayectoria</button>
      </div>
    </div>

    <div class="row cabecera" id="container_profile">
      <!-- Pestañas ficha -->
      <div id="exTab1" class="col-sm-12">
        <ul class="nav nav-tabs">
          {{#view_general_tab_sp}}
          <li id="general_li" class="active">
            <a href="#general_tab" data-toggle="tab">GENERAL</a>
          </li>
          {{/view_general_tab_sp}} {{#view_socioed_tab_sp}}
          <li id="socioed_li">
            <a href="#socioed_tab" data-toggle="tab">SOCIOEDUCATIVO</a>
          </li>
          {{/view_socioed_tab_sp}} {{#view_academic_tab_sp}}
          <li id="academic_li">
            <a href="#academic_tab" data-toggle="tab">ACADÉMICO</a>
          </li>
          {{/view_academic_tab_sp}} {{#view_geographic_tab_sp}}
          <li id="geographic_li">
            <a href="#geographic_tab" data-toggle="tab">GEOGRÁFICO</a>
          </li>{{/view_geographic_tab_sp}}
        </ul>
        <div class="tab-content clearfix ">
          {{#view_general_tab_sp}} {{> block_ases/view_general_tab_sp}} {{/view_general_tab_sp}} {{#view_socioed_tab_sp}} {{> block_ases/view_socioed_tab_sp}}
          {{/view_socioed_tab_sp}} {{#view_academic_tab_sp}}
          <!--academic tab-->
          <div class="tab-pane" id="academic_tab">

            <div class="accordion" role="tablist" aria-multiselectable="true">
              <div class="row" style="padding: 0px 10px 15px ">
                <div class="col-md-3">Promedio Ponderado:
                  <p class="prom" style="display: inline">
                    <b>{{promedio}}</b>
                  </p>
                </div>
                <div class="col-md-3">Estimulos academicos:
                  <p class="est" style="display: inline">
                    <b>{{estimulos}}</b>
                  </p>
                </div>
                <div class="col-md-3">Bajos rendimientos:
                  <p class="baj" style="display: inline">
                    <b>{{bajos}}</b>
                  </p>
                </div>
              </div>
              <div class="panel panel-default" id="panelActual">
                <div class="academic panel-heading" role="tab" id="headingOne">
                  <h4 class="panel-title">
                    <a id="sem_act_title" class="" data-toggle="collapse" data-parent="#accordion" href="#collapseAct" aria-expanded="true"
                      aria-controls="collapseAct">
                      Semestre Actual
                    </a>
                  </h4>
                </div>
                <div id="collapseAct" class="panel-collapse form-pilos in" role="tabpanel" aria-labelledby="headingOne">
                  <div class="panel-group" id="accordion_academic">
                    {{{academic_semester_act}}}
                  </div>
                </div>
              </div>
              {{{historic_academic}}}
            </div>
          </div>
          {{/view_academic_tab_sp}}

          <!-- End academic tab -->

          {{#view_geographic_tab_sp}} {{> block_ases/view_geographic_tab_sp}} {{/view_geographic_tab_sp}}

        </div>
      </div>

    </form>
  </div>

  <!-- MODALES DE SEGUIMIENTO -->
  <div class="mymodal" id="modal_v2_peer_tracking" name="modal_v2_pares">
    <div class="outside" ></div>
    <div class="mymodal-content">
    
      <div class="mymodal-header col-sm-12">
        <div class="col-sm-11">
          <h4 class="" id="myModalLabel">Nuevo registro</h4>
        </div>
        <div class="col-sm-1">
          <span id="mymodal-v2-close" class="mymodal-close" aria-hidden="true">&times;</span>
        </div>
      </div>
      <div class="mymodal-body mymodal-body_v2 col-sm-12">
        <div class="row">
          <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
            {{{form_seguimientos}}}
          </div>
        </div>
      </div>
      <div class="mymodal-footer">
          .
      </div>
    </div>
    
  </div>
</div>

  <div class="mymodal" id="modal_v2_edit_peer_tracking" name="modal_edit_v2_pares">
    <div class="outside" ></div>
    <div class="mymodal-content">
    
      <div class="mymodal-header col-sm-12">
        <div class="col-sm-11">
          <h4 class="" id="myModalLabel">Visualización/Edición</h4>
        </div>
        <div class="col-sm-1">
          <span id="mymodal-v2-close" class="mymodal-close" aria-hidden="true">&times;</span>
        </div>
      </div>
      <div class="mymodal-body mymodal-body_v2 col-sm-12">
        <div class="row">
          <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
            <div id="body_editor"></div>
          </div>
        </div>
      </div>
      <div class="mymodal-footer">
          .
      </div>
    </div>
    
  </div>
</div>
<!-- FIN MODALES SEGUIMIENTO V2 -->

<!-- Modal primer acercamiento -->
<div class="mymodal" id="modal_primer_acercamiento" name="modal_primer_acercamiento_name">
    <div class="outside" ></div>
    <div class="mymodal-content">
      <div class="mymodal-header col-sm-12">
        <div class="col-sm-11">
          <h4 class="" id="myModalLabel">Visualización/Edición</h4>
        </div>
        <div class="col-sm-1">
          <span id="mymodal-v2-close" class="mymodal-close" aria-hidden="true">&times;</span>
        </div>
      </div>
      <div class="mymodal-body mymodal-body_v2 col-sm-12">
        <div class="row">
          <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
            <div id="primer_acercamiento_form">{{{primer_acercamiento}}}</div>
          </div>
        </div>
      </div>
      <div class="mymodal-footer">
          .
      </div>
    </div>
    
  </div>
</div>
<!-- end primer acercamiento -->

<!-- Modal seguimiento geográfico -->
<div class="mymodal" id="modal_seguimiento_geografico" name="modal_seguimiento_geografico_name">
    <div class="outside" ></div>
    <div class="mymodal-content">
      <div class="mymodal-header col-sm-12">
        <div class="col-sm-11">
          <h4 class="" id="myModalLabel">Nuevo registro</h4>
        </div>
        <div class="col-sm-1">
          <span id="mymodal-v2-close" class="mymodal-close" aria-hidden="true">&times;</span>
        </div>
      </div>
      <div class="mymodal-body mymodal-body_v2 col-sm-12">
        <div class="row">
          <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
            <div id="seguimiento_geografico_form">{{{form_seguimientos_geograficos}}}</div>
          </div>
        </div>
      </div>
      <div class="mymodal-footer">
          .
      </div>
    </div>
  </div>
  <!-- end primer acercamiento -->

  <!-- Modal Seguimiento pares-->
  <div class="mymodal" id="modal_peer_tracking" name="modal_pares">
    <div class="mymodal-content">
      <div class="mymodal-header col-sm-12">
        <div class="col-sm-11">
          <h4 class="" id="myModalLabel">Seguimiento de pares</h4>
        </div>
        <div class="col-sm-1">
          <span class="mymodal-close" aria-hidden="true">&times;</span>
        </div>
      </div>
      <div class="mymodal-body col-sm-12">
        <form class="form-horizontal" id="tracking_peer_form" role="form">

          <input type="text" id="id_tracking_peer" name="id_tracking_peer" class="hide" />

          <div class="row form-legend">
            <h4>Datos generales</h4>
          </div>
          <!-- Fecha Lugar-->
          <div class="row">
            <div class="col-sm-6">
              <div class="form-group">
                <label for="date" class="col-sm-3">Fecha (aaaa-mm-dd)
                  <b style="color:red;">*</b>
                </label>
                <div class="col-sm-9">
                  <input type="text" maxlength="10" class="form-control" id="date" name="date" />
                </div>
              </div>
            </div>
            <div class="col-sm-6">
              <div class="form-group">
                <label for="place" class="col-sm-3">Lugar
                  <b style="color:red;">*</b>
                </label>
                <div class="col-sm-9">
                  <input type="text" class="form-control" id="place" name="place">
                </div>
              </div>
            </div>
          </div>

          <!-- Hora inicio - Hora finalización-->
          <div class="row">
            <div class="col-sm-6">
              <div class="form-group">
                <label for="h_ini" class="col-sm-3">Hora de inicio
                  <b style="color:red;">*</b>
                </label>
                <div class="col-sm-3">
                  <select class="form-control" id="h_ini" name="h_ini"></select>
                </div>
                <label class="col-sm-1" for="m_ini">:</label>
                <div class="col-sm-3">
                  <select class="form-control" id="m_ini" name="m_ini"></select>
                </div>
              </div>
            </div>

            <div class="col-sm-6">
              <div class="form-group">
                <label for="h_fin" class="col-sm-3">Hora de finalización
                  <b style="color:red;">*</b>
                </label>
                <div class="col-sm-3">
                  <select class="form-control" id="h_fin" name="h_fin"></select>
                </div>
                <span class="col-sm-1">:</span>
                <div class="col-sm-3">
                  <select class="form-control" id="m_fin" name="m_fin"></select>
                </div>
              </div>
            </div>
          </div>

          <!-- Tema - Objetivos-->
          <div class="row">
            <div class="col-sm-6">
              <div class="form-group">
                <label for="tema" class="col-sm-12">Tema
                  <b style="color:red;">*</b>
                </label>
                <div class="col-sm-12">
                  <textarea class="form-control" id="topic_textarea" name="tema" maxlength="1000"></textarea>
                </div>
              </div>
            </div>
            <div class="col-sm-6">
              <div class="form-group">
                <label for="objetivos" class="col-sm-12">Objetivos
                  <b style="color:red;">*</b>
                </label>
                <div class="col-md-12">
                  <textarea class="form-control" id="objetivos" name="objetivos" maxlength="1000"></textarea>
                </div>
              </div>
            </div>
          </div>

          <!--<div class="row hide" id="infomonitor">-->
          <!--  <label for="monitor" class="form-control-label col-xs-2 col-sm-2">Monitor guía</label>-->
          <!--  <span class="col-sm-4" id="monitor"></span>-->
          <!--</div>-->

          <div class="row form-legend">
            <h4>Actividades</h4>
          </div>

          <!-- Individual - Familiar -->
          <div class="row">
            <div class="col-sm-6">
              <div class="form-group">
                <label for="individual" class="col-sm-12">Individual</label>
                <div class="col-sm-12">
                  <textarea class="form-control" id="individual" name="individual" maxlength="5000"></textarea>
                </div>
              </div>
              <div class="form-group" id="riesgo_ind_div">
                <div class="col-sm-10" id="radio_div">
                  <label class="radio-inline">
                    <input type="radio" name="riesgo_ind" value="1">Bajo
                  </label>
                  <label class="radio-inline">
                    <input type="radio" name="riesgo_ind" value="2">Medio
                  </label>
                  <label class="radio-inline">
                    <input type="radio" id="high_individual_risk" name="riesgo_ind" value="3" />Alto
                  </label>
                  <label class="radio-inline">
                    <span style="color:gray;" class="glyphicon glyphicon-erase" id="clean_individual_risk"></span>
                  </label>
                  <label class="radio-inline">
                    <input type='radio' id='no_value_individual' name='riesgo_ind' value='0' checked/>
                  </label>
                </div>
              </div>
            </div>

            <div class="col-sm-6">
              <div class="form-group">
                <label for="familiar" class="col-sm-12">Familiar</label>
                <div class="col-sm-12">
                  <textarea class="form-control small" id="familiar" name="familiar" maxlength="5000"></textarea>
                </div>
              </div>
              <div class="form-group" id="riesgo_familia_div">
                <div class="col-sm-10" id="radio_div">
                  <label class="radio-inline" id="1">
                    <input type="radio" name="riesgo_familiar" value="1">Bajo
                  </label>
                  <label class="radio-inline" id="3">
                    <input type="radio" name="riesgo_familiar" value="2">Medio
                  </label>
                  <label class="radio-inline" id="5">
                    <input type='radio' id='high_familiar_risk' name='riesgo_familiar' value="3">Alto
                  </label>
                  <label class="radio-inline">
                    <span style="color:gray;" class="glyphicon glyphicon-erase" id="clean_familiar_risk"></span>
                  </label>
                  <label class="radio-inline">
                    <input type='radio' id='no_value_familiar' name='riesgo_familiar' value='0' checked/>
                  </label>
                </div>
              </div>
            </div>
          </div>

          <!-- Académico - Económico -->
          <div class="row">
            <div class="col-sm-6">
              <div class="form-group">
                <label for="academico" class="col-sm-12">Académico</label>
                <div class="col-sm-12">
                  <textarea class="form-control small" id="academico" name="academico" maxlength="5000"></textarea>
                </div>
              </div>
              <div class="form-group" id="riesgo_aca_div">
                <div class="col-sm-10" id="radio_div">
                  <label class="radio-inline" id="1">
                    <input type="radio" name="riesgo_aca" value="1">Bajo
                  </label>
                  <label class="radio-inline" id="2">
                    <input type="radio" name="riesgo_aca" value="2">Medio
                  </label>
                  <label class="radio-inline" id="3">
                    <input type='radio' id='high_academic_risk' name='riesgo_aca' value="3">Alto
                  </label>
                  <label class="radio-inline">
                    <span style="color:gray;" class="glyphicon glyphicon-erase" id="clean_academic_risk"></span>
                  </label>
                  <label class="radio-inline">
                    <input type='radio' id='no_value_academic' name='riesgo_aca' value='0' checked />
                  </label>
                </div>
              </div>
            </div>

            <div class="col-sm-6">
              <div class="form-group">
                <label for="economico" class="col-sm-12">Económico</label>
                <div class="col-sm-12">
                  <textarea class="form-control small" id="economico" name="economico" maxlength="5000"></textarea>
                </div>
              </div>
              <div class="form-group" id="riesgo_econom_div">
                <div class="col-sm-10" id="radio_div">
                  <label class="radio-inline" id="1">
                    <input type="radio" name="riesgo_econom" value="1">Bajo
                  </label>
                  <label class="radio-inline" id="2">
                    <input type="radio" name="riesgo_econom" value="2">Medio
                  </label>
                  <label class="radio-inline" id="3">
                    <input type='radio' id='high_economic_risk' name='riesgo_econom' value="3">Alto
                  </label>
                  <label class="radio-inline">
                    <span style="color:gray;" class="glyphicon glyphicon-erase" id="clean_economic_risk"></span>
                  </label>
                  <label class="radio-inline">
                    <input type='radio' id='no_value_economic' name='riesgo_econom' value='0' checked/>
                  </label>
                </div>
              </div>
            </div>
          </div>

          <!-- Vida universitaria -->
          <div class="row">
            <div class="col-sm-6">
              <div class="form-group">
                <label for="vida_uni" class="col-sm-12">Vida universitaria y ciudad</label>
                <div class="col-sm-12">
                  <textarea class="form-control small" id="vida_uni" name="vida_uni" maxlength="5000"></textarea>
                </div>
              </div>
              <div class="form-group" id="riesgo_uni_div">
                <div class="col-sm-10" id="radio_div">
                  <label class="radio-inline" id="1">
                    <input type="radio" name="riesgo_uni" value="1">Bajo
                  </label>
                  <label class="radio-inline" id="2">
                    <input type="radio" name="riesgo_uni" value="2">Medio
                  </label>
                  <label class="radio-inline" id="3">
                    <input type='radio' id='high_life_risk' name='riesgo_uni' value="3">Alto
                  </label>
