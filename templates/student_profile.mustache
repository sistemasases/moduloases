{{#data}}
    {{> block_ases/side_menu}} {{{message}}}
    <div class="container">
        <div id="custom_metadata" data-view-name="student_profile"></div>
        {{#sistemas}}<div id="dphpforms_role_support" data-info="sistemas" style="display:none;"></div>{{/sistemas}}
        {{#dir_socioeducativo}}<div id="dphpforms_role_support" data-info="dir_socioeducativo" style="display:none;"></div>{{/dir_socioeducativo}}
        {{#monitor_ps}}<div id="dphpforms_role_support" data-info="monitor_ps" style="display:none;"></div>{{/monitor_ps}}
        <div id="dphpforms_ases_student_code" data-info="{{ases_student_code}}" style="display:none;"></div>
        <div id="dphpforms_block_instance" data-info="{{instance}}" style="display:none;"></div>
        <div id="dphpforms_block_courseid" data-info="{{courseid}}" style="display:none;"></div>
        <div id="dphpforms_monitor_id" data-info="{{monitor_id}}" style="display:none;"></div>
        <div id="dphpforms_practicing_id" data-info="{{practicing_id}}" style="display:none;"></div>
        <div id="dphpforms_professional_id" data-info="{{professional_id}}" style="display:none;"></div>
        <div id="dphpforms_username" data-info="{{student_username}}" style="display:none;"></div>
        <div id="dphpforms_fullname" data-info="{{student_fullname}}" style="display:none;"></div>
        <div id="risk_graphic_info" data-info="" style="display:none;"></div>
        <input type="text" class="hide" id="current_user_id" value="{{id_dphpforms_creado_por}}"/>

        <form id="ficha_estudiante">
            <input type="text" class="hide" id="id_ases" name="id_ases" value="{{id_ases}}"/>
            <input type="text" class="hide" id="id_moodle" name="id_moodle" value="{{id_moodle}}"/>

            <div id="div-header-profile" class="ases-row">
                <div id="div-header-info" class="ases-col-xs-10 ases-col-sm-10 ases-col-md-10 ases-col-lg-10">
                    <!-- Select student -->
                    <div id="div-code-student" class="ases-row">
                        <div id="div_select_student" class="ases-col-xs-11 ases-col-sm-11 ases-col-md-11 ases-col-lg-11">{{{code}}}</div>
                        <div id="div-instance-icon" class="ases-col-sm-1 ases-col-lg-1">
                            <span {{#update_ases_status_sp}}id="icon-tracking" {{/update_ases_status_sp}} class="ases-glyphicon ases-glyphicon-ok i-tracking{{#ases_status_on_tracking}}-t{{/ases_status_on_tracking}}
                                tooltip-v1 ases-col-sm-6 ases-col-lg-6" data-toggle="tooltip" data-placement="top" title="{{ases_status_description}}">
                            </span>
                            <input type="hidden" id="input_status_ases" value="{{ases_status}}">
                            <span>ASES</span>
                        </div>
                    </div>
                    <!-- End select student -->

                    <div id="row-general-fields" class="ases-row">
                        <div id="div_doc_type" class="ases-col-xs-1 ases-col-sm-1 ases-col-md-1 ases-col-lg-1 div_general_fields">
                            <select  id="tipo_doc" class="ases-form-control" name="tipo_doc" disabled>
                                {{#document_type}}
                                    <option value="{{nombre}}" {{selected}}>{{nombre}}</option>
                                {{/document_type}}
                            </select>
                        </div>

                        <div id="div_input_cc" class="ases-col-xs-1 ases-col-sm-1 ases-col-md-1 ases-col-lg-1 div_general_fields">
                            <input type="text" class="ases-form-control" name="num_doc" id="num_doc" value="{{num_doc}}" maxlength="12" readonly>
                        </div>

                        <div id="div_age" class="ases-col-xs-1 ases-col-sm-1 ases-col-md-1 ases-col-lg-1 div-general-fields">
                            <span id="age">{{age}} años</span>
                            <input type="text" id="birthdate" class="brithdate_picker ases-form-control" name="fecha_nac" maxlength="10" value="{{birthdate}}"/>
                        </div>

                        <div id="div_email_moodle" class="ases-col-xs-5 ases-col-sm-5 ases-col-md-5 ases-col-lg-5 div-general-fields">
                            <input type="email" id="email" class="ases-form-control" name="email" value="{{email_moodle}}" readonly>
                        </div>

                        <div id="div_span_icetex_status" class="ases-col-xs-1 ases-col-sm-1 ases-col-md-1 ases-col-lg-1 div-general-fields">
                            <span id="span-icetex-status">ICETEX</span>
                        </div>

                        <div id="div_icetex_status" class="ases-col-xs-2 ases-col-sm-2 ases-col-md-2 ases-col-lg-2 div-general-fields">
                            {{#update_icetex_status_sp}}
                                <select id="icetex_status" class="ases-form-control" disabled>
                                    {{{options_status_icetex}}}
                                </select>
                            {{/update_icetex_status_sp}} {{^update_icetex_status_sp}}
                            <span id="span-icetex-name" class="ases-col-sm-2 ases-col-lg-2 tooltip-v1" data-toggle="tooltip" data-placement="top" title="{{icetex_status_name}}">
                                {{icetex_status_description}}
                            </span>
                        {{/update_icetex_status_sp}}
                        </div>
                    </div>

                    <div class="ases-row" id="div-hr"><hr class="hr-general-fields"></div>

                    <div class="ases-col-xs-12 ases-col-sm-8 ases-col-md-8 ases-col-lg-8" id="div_academic_programs">
                        <h4>Programas académicos</h4>
                        <table class="table table-hover borderless">
                            <tbody>
                                {{#academic_programs}}
                                    <tr id="tr-{{id_moodle_user}}">
                                        <td class="ases-col-sm-8 ases-col-lg-8 td-middle" id="{{academic_program_id}}">
                                            {{{cod_univalle}}} - {{{nombre_programa}}} - {{{nombre_sede}}}
                                        </td>

                                        <td class="ases-col-sm-3 ases-col-lg-3">
                                            {{#update_program_status_sp}}
                                                <select class="select_statuses_program" id="select-{{academic_program_id}}"></select>
                                            {{/update_program_status_sp}}

                                            {{^update_program_status_sp}}
                                                <select class="select_statuses_program" id="select-{{academic_program_id}}" disabled>
                                                    {{#statuses}}
                                                        <option value={{id}} {{selected}}>{{nombre}}</option>
                                                    {{/statuses}}
                                                </select>
                                            {{/update_program_status_sp}}
                                        </td>
                                        <td class="ases-col-sm-1 ases-col-lg-1 td-middle">
                                            {{#update_tracking_status_sp}}
                                                <label class="switch">
                                                    <input class="input-tracking" type="checkbox" id="div_flags_{{academic_program_id}}" disabled>
                                                    <div class="slider round"></div>
                                                </label>
                                            {{/update_tracking_status_sp}}
                                        </td>
                                    </tr>
                                {{/academic_programs}}
                            </tbody>
                        </table>
                        {{^academic_programs}} No registra programas académicos. {{/academic_programs}}
                    </div>

                    <div class="ases-col-xs-12 ases-col-sm-3 ases-col-md-3 ases-col-lg-3" id="div_cohorts">
                        <h4>Cohortes</h4>
                        <table class="table table-sm table-hover borderless" id="cohorts_table">
                            <tbody>
                                {{#student_cohorts}}
                                    <tr>
                                        <td class="ases-col-xs-12 ases-col-sm-12 ases-col-lg-12 td-middle">
                                            {{name}}
                                        </td>
                                    </tr>
                                {{/student_cohorts}}
                            </tbody>
                        </table>
                        <div id="cond_excepcion_name" hidden>
                            {{condition_excepcion}}
                        </div>

                        <div id="cond_excepcion_alias" hidden>
                            {{condition_excepcion_alias}}
                        </div>
                        {{^student_cohorts}} No registra cohortes asociadas. {{/student_cohorts}}
                    </div>
                    {{#update_general_tab_sp}}
                        <div class="ases-col-xs-1 ases-col-sm-1 ases-col-md-1 ases-col-lg-1" id="div-icon-edit">
                            <span class="ases-col-sm-12 ases-col-lg-12 tooltip-v1" id="span-icon-edit" data-toggle="tooltip" data-placement="top" title="Editar ficha"></span>
                            <span class="ases-col-sm-6 ases-col-lg-6 tooltip-v1" id="span-icon-save-profile" data-toggle="tooltip" data-placement="top" title="Guardar cambios" hidden></span>
                            <span class="ases-col-sm-6 ases-col-lg-6 tooltip-v1" id="span-icon-cancel-edit" data-toggle="tooltip" data-placement="top" title="Cancelar edición" hidden></span>
                        </div>
                    {{/update_general_tab_sp}}
                </div>
                <!-- Profile image html, see $OUTPUT->user_picture -->
                <div class="ases-col-xs-2 ases-col-sm-2 ases-col-md-2 ases-col-lg-2  {{#show_html_elements_update_user_profile_image}}profile-image-can-update{{/show_html_elements_update_user_profile_image}} profile-image">
                    {{{html_profile_image}}}
                    {{#show_html_elements_update_user_profile_image}}
                        <div class="open-profile-image">
                            <button type=button id="view_form_update_profile_image" class="open-button"> Cambiar imágen de perfil</button>
                        </div>
                    {{/show_html_elements_update_user_profile_image}}
                </div>
            </div>

            <!-- Semaforo de riesgos -->
            <div class="ases-row div_risk">
                <div class="ases-col-sm-1 ases-col-lg-1 no-padding-left">
                    <div id="div_title_risk">
                        <b>RIESGOS:</b>
                    </div>
                </div>

                <div class="ases-col-sm-9 ases-col-lg-9 no-padding-left" id="div_bar_risks">
                    <div class="{{individual_class}} ases-col-sm-2 ases-col-lg-2" data-risk-level="{{level_individual}}" id="individual_risk">
                        <span><b>INDIVIDUAL</b></span>
                    </div>
                    <div hidden>{{individual_risk}}</div>

                    <div class="{{familiar_class}} ases-col-sm-2 ases-col-lg-2" data-risk-level="{{level_familiar}}" id="familiar_risk">
                        <b> <span>FAMILIAR</span></b>
                    </div>
                    <div hidden>{{familiar_risk}}</div>

                    <div class="{{academic_class}} ases-col-sm-2 ases-col-lg-2" data-risk-level="{{level_academic}}" id="academic_risk">
                        <b><span>ACADÉMICO</span></b>
                    </div>
                    <div hidden>{{academic_risk}}</div>

                    <div class="{{economic_class}} ases-col-sm-2 ases-col-lg-2" data-risk-level="{{level_economic}}" id="economic_risk">
                        <b><span>ECONÓMICO</span></b>
                    </div>
                    <div hidden>{{economic_risk}}</div>

                    <div class="{{life_class}} ases-col-sm-2 ases-col-lg-2" data-risk-level="{{level_life}}" id="life_risk">
                        <b>VIDA UNIV.</b>
                    </div>
                    <div hidden>{{life_risk}}</div>

                    <div class="{{geographic_class}} ases-col-sm-2 ases-col-lg-2" data-risk-level="{{geographic_risk_level}}" id="geo_risk">
                        <b><span>GEOGRÁFICO</span></b>
                    </div>
                    <div hidden>{{geographic_risk_level}}</div>
                </div>

                <div class="ases-col-sm-2 ases-col-lg-2" id="div_buttons_risk">
                    <button class="risk_graph_button ases-col-sm-6 ases-col-lg-6" type="button" id="view_graph_radial_button">Radial</button>
                    <button class="risk_graph_button ases-col-sm-6 ases-col-lg-6" type="button" id="view_graphic_risk_button">Trayectoria</button>
                </div>
            </div>

            <div class="ases-row cabecera" id="container_profile">
                <!-- Pestañas ficha -->
                <div id="exTab1" class="ases-col-sm-12 ases-col-lg-12">
                    <ul class="ases-nav ases-nav-pills" id="nav-linea">
                        {{#view_general_tab_sp}}
                            <li id="general_li" class="ases-active">
                                <a href="#general_tab" data-toggle="ases-pill">GENERAL</a>
                            </li>
                        {{/view_general_tab_sp}}

                        {{#view_socioed_tab_sp}}
                            <li id="socioed_li">
                                <a href="#socioed_tab" data-toggle="ases-pill">SOCIOEDUCATIVO</a>
                            </li>
                        {{/view_socioed_tab_sp}}

                        {{#view_academic_tab_sp}}
                            <li id="academic_li">
                                <a href="#academic_tab" data-toggle="ases-pill">ACADÉMICO</a>
                            </li>
                        {{/view_academic_tab_sp}}

                        {{#view_geographic_tab_sp}}
                            <li id="geographic_li">
                                <a href="#geographic_tab" data-toggle="ases-pill">GEOGRÁFICO</a>
                            </li>
                        {{/view_geographic_tab_sp}}

                        {{#view_others_tab_sp}}
                            <li id="tracing_others_li">
                                <a href="#tracing_others_tab" data-toggle="ases-pill">CARACTERIZACIÓN</a>
                            </li>
                        {{/view_others_tab_sp}}
                    </ul>
                    <div class="ases-tab-content clearfix">
                        {{#view_general_tab_sp}} {{> block_ases/view_general_tab_sp}} {{/view_general_tab_sp}}
                        {{#view_others_tab_sp}} {{> block_ases/view_others_tab_sp}} {{/view_others_tab_sp}}
                    </div>
                </div>
            </div>
        </form>
    </div>

    <!-- MODALES DE SEGUIMIENTO -->
    <div class="mymodal" id="modal_v2_peer_tracking" name="modal_v2_pares">
        <div class="outside"></div>
        <div class="mymodal-content">
            <div class="mymodal-header col-sm-12">
                <div class="col-xs-9 col-sm-9 col-md-9 col-lg-9">
                    <h4 class="" id="myModalLabel">Nuevo registro</h4>
                </div>

                <div class="col-xs-2 col-sm-2 col-md-2 col-lg-2">
                    <div data-info="seguimiento" class="btn-inasistencia open-inasistencia btn btn-md btn-default">
                        Registrar inasistencia
                    </div>
                </div>

                <div class="col-xs-1 col-sm-1 col-md-1 col-lg-1">
                    <span id="mymodal-v2-close" class="mymodal-close" aria-hidden="true">&times;</span>
                </div>
            </div>

            <div class="mymodal-body mymodal-body_v2 col-sm-12">
                <div class="row">
                    <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
                        {{{form_seguimientos}}}
                    </div>
                </div>
            </div>

            <div class="mymodal-footer">.</div>
        </div>
    </div>

    <div class="mymodal" id="modal_v2_edit_peer_tracking" name="modal_edit_v2_pares">
        <div class="outside"></div>
        <div class="mymodal-content">
            <div class="mymodal-header col-sm-12">
                <div class="col-sm-11">
                    <h4 class="" id="myModalLabel">Visualización/Edición</h4>
                </div>

                <div class="col-sm-1">
                    <span id="mymodal-v2-close" class="mymodal-close" aria-hidden="true">&times;</span>
                </div>
            </div>

            <div class="mymodal-body mymodal-body_v2 col-sm-12">
                <div class="row">
                    <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
                        <div id="body_editor"></div>
                    </div>
                </div>
            </div>

            <div class="mymodal-footer">.</div>
        </div>
    </div>
    <!-- FIN MODALES SEGUIMIENTO V2 -->

    <!-- MODALES DE INASISTENCIAS -->
    <div class="mymodal" id="modal_inasistencia" name="modal_inasistencia">
        <div class="outside"></div>
        <div class="mymodal-content">

            <div class="mymodal-header col-sm-12">
                <div class="col-xs-9 col-sm-9 col-md-9 col-lg-9">
                    <h4 class="" id="myModalLabel">Nuevo registro</h4>
                </div>
                <div class="col-xs-2 col-sm-2 col-md-2 col-lg-2">
                    <div data-info="inasistencia" class="btn-inasistencia open-inasistencia btn btn-md btn-default">
                        Regresar a seguimiento
                    </div>
                </div>
                <div class="col-xs-1 col-sm-1 col-md-1 col-lg-1">
                    <span id="mymodal-v2-close" class="mymodal-close" aria-hidden="true">&times;</span>
                </div>
            </div>
            <div class="mymodal-body mymodal-body_v2 col-sm-12">
                <div class="row">
                    <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
                        {{{form_inasistencia}}}
                    </div>
                </div>
            </div>
            <div class="mymodal-footer">.</div>
        </div>
    </div>

    <div class="mymodal" id="modal_edit_inasistencia" name="modal_edit_inasistencia">
        <div class="outside"></div>
        <div class="mymodal-content">

            <div class="mymodal-header col-sm-12">
                <div class="col-sm-11">
                    <h4 class="" id="myModalLabel">Visualización/Edición</h4>
                </div>
                <div class="col-sm-1">
                    <span id="mymodal-v2-close" class="mymodal-close" aria-hidden="true">&times;</span>
                </div>
            </div>
            <div class="mymodal-body mymodal-body_v2 col-sm-12">
                <div class="row">
                    <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
                        <div id="body_editor"></div>
                    </div>
                </div>
            </div>
            <div class="mymodal-footer">.</div>
        </div>
    </div>
    <!-- FIN MODALES SEGUIMIENTO V2 -->

    <!-- Modal primer acercamiento -->
    <div class="mymodal" id="modal_primer_acercamiento" name="modal_primer_acercamiento_name">
        <div class="outside"></div>
        <div class="mymodal-content">
            <div class="mymodal-header col-sm-12">
                <div class="col-sm-11">
                    <h4 class="" id="myModalLabel">Visualización/Edición</h4>
                </div>
                <div class="col-sm-1">
                    <span id="mymodal-v2-close" class="mymodal-close" aria-hidden="true">&times;</span>
                </div>
            </div>
            <div class="mymodal-body mymodal-body_v2 col-sm-12">
                <div class="row">
                    <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
                        <div id="primer_acercamiento_form">{{{primer_acercamiento}}}</div>
                    </div>
                </div>
            </div>
            <div class="mymodal-footer">.</div>
        </div>
    </div>
    <!-- end primer acercamiento -->

    <!-- Modal seguimiento geográfico -->
    <div class="mymodal" id="modal_seguimiento_geografico" name="modal_seguimiento_geografico_name">
        <div class="outside"></div>
        <div class="mymodal-content">
            <div class="mymodal-header col-sm-12">
                <div class="col-sm-11">
                    <h4 class="" id="myModalLabel">Nuevo registro</h4>
                </div>
                <div class="col-sm-1">
                    <span id="mymodal-v2-close" class="mymodal-close" aria-hidden="true">&times;</span>
                </div>
            </div>
            <div class="mymodal-body mymodal-body_v2 col-sm-12">
                <div class="row">
                    <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
                        <div id="seguimiento_geografico_form">{{{form_seguimientos_geograficos}}}</div>
                    </div>
                </div>
            </div>
            <div class="mymodal-footer">.</div>
        </div>
    </div>
    <!-- end primer acercamiento -->

    <!-- Modal Seguimiento pares-->
    <div class="mymodal" id="modal_peer_tracking" name="modal_pares">
        <div class="mymodal-content">
            <div class="mymodal-header col-sm-12">
                <div class="col-sm-11">
                    <h4 class="" id="myModalLabel">Seguimiento de pares</h4>
                </div>

                <div class="col-sm-1">
                    <span class="mymodal-close" aria-hidden="true">&times;</span>
                </div>
            </div>

            <div class="mymodal-body col-sm-12">
                <form class="form-horizontal" id="tracking_peer_form" role="form">
                    <input type="text" id="id_tracking_peer" name="id_tracking_peer" class="hide"/>

                    <div class="row form-legend">
                        <h4>Datos generales</h4>
                    </div>

                    <!-- Fecha Lugar-->
                    <div class="row">
                        <div class="col-sm-6">
                            <div class="form-group">
                                <label for="date" class="col-sm-3">Fecha (aaaa-mm-dd)
                                    <b style="color:red;">*</b>
                                </label>
                                <div class="col-sm-9">
                                    <input type="text" maxlength="10" class="form-control" id="date" name="date"/>
                                </div>
                            </div>
                        </div>

                        <div class="col-sm-6">
                            <div class="form-group">
                                <label for="place" class="col-sm-3">Lugar
                                    <b style="color:red;">*</b>
                                </label>
                                <div class="col-sm-9">
                                    <input type="text" class="form-control" id="place" name="place">
                                </div>
                            </div>
                        </div>
                    </div>

                    <!-- Hora inicio - Hora finalización-->
                    <div class="row">
                        <div class="col-sm-6">
                            <div class="form-group">
                                <label for="h_ini" class="col-sm-3">
                                    Hora de inicio
                                    <b style="color:red;">*</b>
                                </label>
                                <div class="col-sm-3">
                                    <select class="form-control" id="h_ini" name="h_ini"></select>
                                </div>

                                <label class="col-sm-1" for="m_ini">:</label>
                                <div class="col-sm-3">
                                    <select class="form-control" id="m_ini" name="m_ini"></select>
                                </div>
                            </div>
                        </div>

                        <div class="col-sm-6">
                            <div class="form-group">
                                <label for="h_fin" class="col-sm-3">Hora de finalización
                                    <b style="color:red;">*</b>
                                </label>

                                <div class="col-sm-3">
                                    <select class="form-control" id="h_fin" name="h_fin"></select>
                                </div>

                                <span class="col-sm-1">:</span>
                                <div class="col-sm-3">
                                    <select class="form-control" id="m_fin" name="m_fin"></select>
                                </div>
                            </div>
                        </div>
                    </div>

                    <!-- Tema - Objetivos-->
                    <div class="row">
                        <div class="col-sm-6">
                            <div class="form-group">
                                <label for="tema" class="col-sm-12">Tema
                                    <b style="color:red;">*</b>
                                </label>
                                <div class="col-sm-12">
                                    <textarea class="form-control" id="topic_textarea" name="tema"
                                              maxlength="1000"></textarea>
                                </div>
                            </div>
                        </div>

                        <div class="col-sm-6">
                            <div class="form-group">
                                <label for="objetivos" class="col-sm-12">Objetivos
                                    <b style="color:red;">*</b>
                                </label>
                                <div class="col-md-12">
                                    <textarea class="form-control" id="objetivos" name="objetivos"
                                              maxlength="1000"></textarea>
                                </div>
                            </div>
                        </div>
                    </div>

                    <!--<div class="row hide" id="infomonitor">-->
                    <!--  <label for="monitor" class="form-control-label col-xs-2 col-sm-2">Monitor guía</label>-->
                    <!--  <span class="col-sm-4" id="monitor"></span>-->
                    <!--</div>-->

                    <div class="row form-legend">
                        <h4>Actividades</h4>
                    </div>

                    <!-- Individual - Familiar -->
                    <div class="row">
                        <div class="col-sm-6">
                            <div class="form-group">
