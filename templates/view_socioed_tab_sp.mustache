<div class="tab-pane" id="socioed_tab">
    <div id="peer_tracking_info" data-info="{{peer_tracking_v3_string}}" style="display:none;"></div>
    <div id="accordion_socioed" role="tablist" aria-multiselectable="true">
        {{#view_peer_trackings_sp}}
            <div class="panel panel-default" id="panelPares">
                <div class="panel-heading" role="tab" id="headingOneSocioed">
                    <h4 class="panel-title">

                        <a id="seg_pares_title" class="collapsed" data-toggle="collapse" data-parent="#accordion_socioed" href="#collapseOne" aria-expanded="false" aria-controls="collapseOne">
                            Seguimiento de Pares
                        </a>

                    </h4>
                </div>
                <div id="collapseOne" class="panel-collapse collapse in form-pilos" role="tabpanel" aria-labelledby="headingOneSocioed">
                    {{#add_peer_tracking_sp}}
                        <!--<button type="button" id="button_add_track" class="btn-primary">
                         Nuevo seguimiento (2017)
                         </button>-->
                        <button type="button" id="button_add_v2_track" class="btn-primary">Nuevo seguimiento</button>
                        {{#view_button_historic}}
                            <button type="button" id="button_carga_Hisotricos" class="btn-primary">Registros Históricos</button>
                        {{/view_button_historic}}

                    {{/add_peer_tracking_sp}}
                    <div id="socioed_historic"></div>
                </div>
            </div>
        {{/view_peer_trackings_sp}}

        {{#view_groupal_trackings_sp}}
            <div class="panel panel-default" id="panelGrupal">
                <div class="panel-heading" role="tab" id="headingTwo">
                    <h4 class="panel-title">
                        <!-- <a id="seg_grupal_title" class="collapsed" data-toggle="collapse" data-parent="#accordion_socioed" href="#collapseTwo" aria-expanded="false" aria-controls="collapseTwo"> -->
                        <a>
                            Seguimiento Grupal
                        </a>
                    </h4>
                </div>
                <div id="collapseTwo" class="panel-collapse collapse" role="tabpanel" aria-labelledby="headingTwo">
                    <div class="ScrollStyle" id="list_grupal" name="list_grupal"></div>
                </div>
            </div>
        {{/view_groupal_trackings_sp}}

        {{#first_contact_sp}}
            <div class="panel panel-default" id="panelPrimerAcerca">
                <div class="panel-heading" role="tab" id="headingThree">
                    <h4 class="panel-title">
                        <!--<a class="collapsed" data-toggle="collapse" data-parent="#accordion_socioed" href="#collapseThree" aria-expanded="false" aria-controls="collapseThree">-->
                        <a>
                            Primer Acercamiento Profesional
                        </a>
                    </h4>
                </div>
                <div id="collapseThree" class="panel-collapse collapse" role="tabpanel" aria-labelledby="headingThree">
                    {{#registro_primer_acercamiento}}
                        <button type="button" id="button_primer_acercamiento" class="btn-primary">Registrar acercamiento</button>
                    {{/registro_primer_acercamiento}}
                    {{#actualizar_primer_acercamiento}}
                        <button type="button" id="button_actualizar_primer_acercamiento" data-record-id="{{id_primer_acercamiento}}" class="btn-primary">Ver/Actualizar acercamiento</button>
                    {{/actualizar_primer_acercamiento}}
                </div>
            </div>
        {{/first_contact_sp}}

        {{#socioeducational_accompaniment_sp}}
            <div class="panel panel-default" id="panelAcompSocio">
                <div class="panel-heading" role="tab" id="headingFour">
                    <h4 class="panel-title">
                        <!--<a class="collapsed" data-toggle="collapse" data-parent="#accordion_socioed" href="#collapseFour" aria-expanded="false" aria-controls="collapseFour">-->
                        <a>
                            Acompañamiento SocioEducativo
                        </a>
                    </h4>
                </div>
                <div id="collapseFour" class="panel-collapse collapse" role="tabpanel" aria-labelledby="headingFour">
                    <div class="ScrollStyle well" id="list_AcompaSocio" name="list_AcompaSocio"></div>
                    <!-- Button trigger modal -->
                    <button type="submit" id="socioedu_add_AcompaSocio" class="submit hide" data-toggle="modal" data-target="#myModalAcompaSocio">Adicionar nuevo</button>
                </div>
            </div>
        {{/socioeducational_accompaniment_sp}}

        {{#socioeducational_trackings_sp}}
            <div class="panel panel-default" id="panelSegSocio">
                <div class="panel-heading" role="tab" id="headingFive">
                    <h4 class="panel-title">
                        <!-- <a id="seg_socio_title" class="collapsed" data-toggle="collapse" data-parent="#accordion" href="#collapseFive" aria-expanded="false" aria-controls="collapseFive">-->
                        <a>
                            Seguimiento SocioEducativo
                        </a>
                    </h4>
                </div>
                <div id="collapseFive" class="panel-collapse collapse" role="tabpanel" aria-labelledby="headingFive">
                    <div class="ScrollStyle well" id="list_SegSocio" name="list_SegSocio"></div>
                    <!-- Button trigger modal -->
                    <button type="submit" id="socioedu_add_segsocio" class="submit hide" data-toggle="modal" data-target="#myModalSegSocio">Adicionar nuevo</button>
                </div>
            </div>
        {{/socioeducational_trackings_sp}}
    </div>
</div>